<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context.xsd">

    <!--<context:component-scan base-package="vkshell"/>-->

    <bean id="startMode" class="vkshell.commands.DefaultMode.DefaultMode" init-method="init"></bean>
    <bean id="CommandParser" class="vkshell.shell.cmd.tools.CommandParser"></bean>
    <bean id="CommandFactory" class="vkshell.shell.cmd.tools.CommandFactory"></bean>

    <bean id="appmode" scope="prototype"></bean>
    <bean id="command" scope="prototype"></bean>
</beans>